{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "semicolon": false,
    "ter-indent": [true, 4],
    "trailing-comma": [true, {"multiline": {
      "objects": "always",
      "arrays": "always",
      "functions": "never",
      "typeLiterals": "ignore"
    }, "singleline": "never"}]

  }
}
